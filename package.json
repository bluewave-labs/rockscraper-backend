{
  "name": "onboarding",
  "version": "1.0.0",
  "description": "Onboarding app for Bluewave",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.ts",
    "dev": "nodemon --watch \"./src/**\" --ext \"ts,js\" --ignore \"src/**/*.spec.ts,src/**/*.json\" --exec \"ts-node src/index.ts\"",
    "pretest": "NODE_ENV=test bash pretest-script.sh",
    "posttest": "bash posttest-script.sh",
    "test": "NODE_ENV=test nyc mocha --extension js,mjs 'src/test/**/*.test.*'",
    "test:e2e": "npm run pretest && NODE_ENV=test mocha 'src/test/e2e/**/*.test.mjs'",
    "test:unit": "NODE_ENV=test mocha 'src/test/unit/**/*.test.js' --watch",
    "build": "tsc",
    "dockerUp": "NODE_ENV=dev docker-compose --env-file .env.dev up --build -d",
    "dockerDown": "docker-compose down",
    "dockerWatch": "NODE_ENV=dev docker-compose --env-file .env.dev up --build --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/chai": "^5.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "chai": "^4.5.0",
    "chai-http": "^5.1.1",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.0",
    "nyc": "^17.1.0",
    "sinon": "^19.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2",
    "wait-on": "^8.0.1"
  }
}
