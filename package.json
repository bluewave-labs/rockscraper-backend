    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "chai": "^4.5.0",
    "chai-http": "^5.1.1",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "mocha": "^10.8.2",
